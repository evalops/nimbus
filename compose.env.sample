# Required secrets for docker compose stack
NIMBUS_GITHUB_APP_ID=1
NIMBUS_GITHUB_APP_PRIVATE_KEY=replace-with-github-app-private-key
NIMBUS_GITHUB_APP_INSTALLATION_ID=1
NIMBUS_GITHUB_WEBHOOK_SECRET=local-webhook-secret

NIMBUS_AGENT_TOKEN_SECRET=local-agent-secret
NIMBUS_CACHE_SHARED_SECRET=local-cache-secret
NIMBUS_JWT_SECRET=local-jwt-secret
NIMBUS_METADATA_SINK_URL=http://logging-pipeline:8090
NIMBUS_CACHE_METRICS_DB=sqlite+pysqlite:///cache-metrics.db
NIMBUS_DOCKER_CACHE_DB_PATH=sqlite+pysqlite:///docker-cache.db

# Host agent specifics (set after minting via admin CLI)
NIMBUS_CONTROL_PLANE_TOKEN=
NIMBUS_CACHE_TOKEN_VALUE=
